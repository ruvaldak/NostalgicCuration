{
    "id": "nostalgicCuration",
    "version": 7,
    "capabilities": ["FileFilter", "DirFilter"],
    "conf": {
        "sounds": {
           "enable": true,
           "weather": true,
           "openCloseSounds": true,
           "expSound": true,
           "footsteps": true,
           "ough": true,
           "legBreak": true
        },
        "sky": {
            "colors": false,
            "cloudHeight": {
                "type": "slider",
                "default": 110,
                "min": -64,
                "max": 392
            }
        },
        "colormaps": {
            "foliage": false
        },
        "liquids": {
            "water": true,
            "lava": true
        }
    }
}